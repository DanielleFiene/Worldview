<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Redirect to New Domain" stopProcessing="true">
          <match url="(.*)" />
          <action type="Redirect" url="https://www.worldviewinvestmentmanagement.com/{R:1}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
